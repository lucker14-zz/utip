div.footer
    position relative
    flex-flow row wrap
    justify-content space-between
    align-items flex-start
    
    width 100%
    padding-top 20px
    margin-top 100px
    
    border-top 1px solid rgba(13, 43, 101, .1)
    
    &--heart-block
        width 200px
        top 20px
        right 0
        left 0
        margin 0 auto
        
        display flex
        flex-flow row wrap
        justify-content center
        align-items center
        
        div.heart
            background-image url('data:image/svg+xml;base64,')
            width 20px
            height 18px
            margin 0 5px
        
    &--content-half
        flex-flow column wrap
        justify-content flex-start
        align-items flex-start
        
        a.blue
            margin-bottom 14px
            text-decoration underline
        
        &.end
            align-items flex-end
            
        .copyright
            display flex
            flex-flow row wrap
            justify-content space-around
            align-items center

            .logo--small
                margin-left 5px
                background-image url('data:image/svg+xml;base64,')
                width 24px
                height 13px